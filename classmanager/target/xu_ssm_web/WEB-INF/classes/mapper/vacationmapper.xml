<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.just.manager.dao.IVacationMapper" >
    <select id="findByLId"  resultType="com.just.manager.domain.Vacation">
        select * from vacation where lid = '11'
    </select>

    <select id="selectByStuNo"  parameterType="java.lang.String" resultType="vacation">
        select * from vacation where stuno=#{stuNo}
    </select>




</mapper>