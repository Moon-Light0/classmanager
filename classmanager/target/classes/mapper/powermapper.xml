<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.just.manager.dao.IPowerMapper">

    <select id="selectByStuNo" parameterType="java.lang.String" resultType="power">
        select * from power where stuno = #{stuNo}
    </select>

    <insert id="insert" parameterType="power">
        insert into power values(#{pid},#{userId},#{role})
    </insert>


</mapper>